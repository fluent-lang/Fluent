/*
    The Fluent programming language
*/

import "@std::io::print";

fun bcd() {
    println("Hello from bcd");
    println("Hello from bcd again!");
}

fun abc() {
    println("Hello, Fluent! (print)");
    println("It works!");
    bcd();
    println("After bcd");
    println("After bcd (again)");
}

fun main() {
    abc();
    println("After abc");
    println("After abc (again)");
}